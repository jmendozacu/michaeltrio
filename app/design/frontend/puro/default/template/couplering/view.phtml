    function productAddToCartWomenMenForm()
    {
        if (jQuery('#couple_women_product_addtocart_form .womencss select.selectpicker').val() == "") {
            alert("Please Select WOMEN'S Ring size!");
            return;
        }

        if (jQuery('#couple_men_product_addtocart_form .mencss select.selectpicker').val() == "") {
            alert("Please Select MEN'S Ring size!");
            return;
        }

        var womenurl = jQuery('#couple_women_product_addtocart_form').attr('action');
        womenurl = womenurl.replace("checkout/cart", "ajaxproducts/index");

        var womendata = jQuery('#couple_women_product_addtocart_form').serialize();
        womendata += '&isAjax=1&qty=1';
        showLoader();
        try {
            jQuery.ajax({
                url: womenurl,
                dataType: 'json',
                type: 'post',
                data: womendata,
                success: function (response) {
                    parent.initAjax(response, true);
                    parent.jQuery.colorbox.close();
                    //alert(response.status + ": " + response.message);
                }
            });
        } catch (e) {
        }

        var menurl = jQuery('#couple_men_product_addtocart_form').attr('action');
        menurl = menurl.replace("checkout/cart", "ajaxproducts/index");

        var mendata = jQuery('#couple_men_product_addtocart_form').serialize();
        mendata += '&isAjax=1&qty=1';
        showLoader();
        try {
            jQuery.ajax({
                url: menurl,
                dataType: 'json',
                type: 'post',
                data: mendata,
                success: function (response) {
                    hideLoader();
                    parent.initAjax(response, true);
                    parent.jQuery.colorbox.close();
                }
            });
        } catch (e) {
        }

    }

    function hideLoader() {
        jQuery('.spinner-container').attr("style", "font-size: 12px; position: relative; vertical-align: top; z-index: 1; padding: 0px 5px; letter-spacing: 0.05em; color: rgb(255, 255, 255); line-height: 40px; border: 1px solid rgb(153, 174, 194); height: 50px; width: 150px; left: 19%; display: none ! important;");
    }

    function showLoader() {
        jQuery('.spinner-container').attr("style", "font-size: 12px; position: relative; vertical-align: top; z-index: 1; padding: 0px 5px; letter-spacing: 0.05em; color: rgb(255, 255, 255); line-height: 40px; border: 1px solid rgb(153, 174, 194); height: 50px; width: 150px; left: 19%; display: block ! important;");
    }

</script>