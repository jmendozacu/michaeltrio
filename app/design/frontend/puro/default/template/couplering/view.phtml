                    function changeSpecialChar(addval, menwomen)
                    {
                        var lastpreviewval;
                        if (jQuery("." + menwomen + " #ringengraving").val().length < 15)
                        {
                            lastpreviewval = jQuery("." + menwomen + " #ringengraving").val();
                            jQuery("." + menwomen + " #ringengraving").val(lastpreviewval + jQuery("<div>").html(addval).text());
                            jQuery("." + menwomen + " #cart_ring_engraving").val(lastpreviewval + jQuery("<div>").html(addval).text());
                            jQuery("." + menwomen + " #engraving").val('1');
                            renderEngraving(menwomen);
                        }
                    }
                    function renderEngraving(menwomen)
                    {
                        jQuery("." + menwomen + " #previewring").val(jQuery("." + menwomen + " #ringengraving").val());
                        var lastpreviewval = jQuery("." + menwomen + " #ringengraving").val();
                        var char_leng = (15 - lastpreviewval.length);
                        jQuery("." + menwomen + " .remaing_word").text(char_leng + " characters left");
                    }
					
                    function changefont(addpreviewclass, menwomen)
                    {
                        jQuery("." + menwomen + ' #previewring').removeClass('Tunga');
                        jQuery("." + menwomen + ' #previewring').removeClass('TimesNewRoman');
                        jQuery("." + menwomen + ' #previewring').removeClass('LaurenScriptRegular');
                        jQuery("." + menwomen + ' #previewring').removeClass('Harrington');
                        jQuery("." + menwomen + ' #previewring').addClass(addpreviewclass);
                        jQuery("." + menwomen + ' .item-etui').removeClass('seledted');
                        jQuery("." + menwomen + ' #' + addpreviewclass).addClass('seledted');
                        jQuery("." + menwomen + " #ring_font").val(addpreviewclass);

                    }
