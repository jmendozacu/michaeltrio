<script type="text/javascript">
    var urlGoToStep = '<?php echo Mage::getBaseUrl() ?>diamonds/diamond/gotostep/';

    jQuery(document).ready(function ($) {
        jQuery('.go-to-step-one').click(function (e) {
            e.preventDefault();
            var diamondType = jQuery(this).attr('diamond');
            goToStep(1, diamondType);
        });

        jQuery('.go-to-step-two').click(function (e) {
            e.preventDefault();
            goToStep(2);
        });

        jQuery('.go-to-step-one-diamond').click(function (e) {
            e.preventDefault();
            var diamondType = jQuery(this).attr('diamond');
			var diamondCut = jQuery(this).attr('diamond_cut');
            if (diamondType) {
                goToStepDiamond(diamondType,diamondCut);
            }
        });

        jQuery('.go-to-step-two-setting').click(function (e) {
            e.preventDefault();
            var settingType = jQuery(this).attr('setting');

            if (settingType) {
                goToStepSetting(settingType);
            }
        });

        jQuery('.go-to-step-three-wedding').click(function (e) {
            e.preventDefault();
            var weddingType = jQuery(this).attr('wedding');
            var arr = weddingType.split('/');

            if (weddingType) {
                goToStepWedding(arr[0], arr[1]);
            }
        });
        
        jQuery('.go-to-step-one-jewellery-gifts').click(function (e) {
            e.preventDefault();
            var jewellerygiftType = jQuery(this).attr('jewellerygifts');
            var arr = jewellerygiftType.split('/');

            if (jewellerygiftType) {
                goToStepJewellerygift(arr[0], arr[1]);
            }
        });
		        
        jQuery('.go-to-step-one-jewellery-gifts-sale').click(function (e) {
			jQuery('.loader-sidebar').addClass('loader-show');
            e.preventDefault();
            var jewellerygiftType = jQuery(this).attr('jewellerygifts');
            var arr = jewellerygiftType.split('/');

            if (jewellerygiftType) {
                goToStepJewellerygiftsale(arr[0], arr[1]);
            }
        });

    });

    function goToStep(step, diamond) {
        jQuery('.loader-sidebar').addClass('loader-show');
        jQuery.post(urlGoToStep, {step: step, shape: diamond}, function (response) {
            response = JSON.parse(response);
            if (response.status) {

                if (step == 2) {
                    var redirectUrl = '<?php echo Mage::getBaseUrl() ?>engagement-rings.php';
                    redirectUrl += '?step=2';
                    window.location.href = redirectUrl;
                }
                else
                {
                    var redirectUrl = '<?php echo Mage::getBaseUrl() ?>diamond.php';
                    window.location.href = redirectUrl;
                }
            }
        });
    }

    function goToStepSetting(setting) {
        jQuery('.loader-sidebar').addClass('loader-show');
        jQuery.post(urlGoToStep, {step: 2, style: setting}, function (response) {
            response = JSON.parse(response);
            if (response.status) {
                var redirectUrl = '<?php echo Mage::getBaseUrl() ?>engagement-rings.php?style=' + setting;
                window.location.href = redirectUrl;
            }
        });
    }

    function goToStepWedding(maincat, wedding) {
        jQuery('.loader-sidebar').addClass('loader-show');
        jQuery.post(urlGoToStep, {step: 'wedding', maincat: maincat, weddingstyle: wedding}, function (response) {
            response = JSON.parse(response);
            if (response.status) {
                var redirectUrl = '<?php echo Mage::getBaseUrl() ?>wedding.php?maincat=' + maincat + '&weddingstyle=' + wedding;
                window.location.href = redirectUrl;
            }
        });
    }
    
    function goToStepJewellerygift(maincat, jewellerygift) {
        jQuery('.loader-sidebar').addClass('loader-show');
        jQuery.post(urlGoToStep, {step: 'jewellerygift', maincat: maincat, style: jewellerygift}, function (response) {
            response = JSON.parse(response);
            if (response.status) {
			    var redirectUrl = '<?php echo Mage::getBaseUrl() ?>'+response.mainjewelleryurl+'?maincat=' + maincat + '&style=' + jewellerygift;
			    //var redirectUrl = '<?php echo Mage::getBaseUrl() ?>'+response.mainjewelleryurl;
                //var redirectUrl = '<?php echo Mage::getBaseUrl() ?>jewellery-gifts.php?maincat=' + maincat + '&style=' + jewellerygift;
                window.location.href = redirectUrl;
            }
        });
    }
	
	function goToStepJewellerygiftsale(maincat, jewellerygift) {
        jQuery('.loader-sidebar').addClass('loader-show');
		/*var redirectUrl = '<?php echo Mage::getBaseUrl() ?>jewellery-gifts/sale.php';
		alert(redirectUrl);
        window.location.href = redirectUrl;*/
        jQuery.post(urlGoToStep, {step: 'jewellerygiftsale', maincat: maincat, styletype: jewellerygift}, function (response) {
            response = JSON.parse(response);
            if (response.status) {
                var redirectUrl = '<?php echo Mage::getBaseUrl() ?>jewellery-gifts.php?styletype=' + jewellerygift;
                window.location.href = redirectUrl;
            }
        });
    }

    function goToStepDiamond(diamond,diamond_cut) {
        jQuery('.loader-sidebar').addClass('loader-show');
        jQuery.post(urlGoToStep, {step: 1, shape: diamond, cut: diamond_cut}, function (response) {
            response = JSON.parse(response);
            if (response.status) {
                var redirectUrl = '<?php echo Mage::getBaseUrl() ?>diamond.php?shape=' + diamond;
                window.location.href = redirectUrl;
            }
        });
    }

</script>