<script type="text/javascript">
    var urlGoToStep = '<?php echo Mage::getBaseUrl() ?>diamonds/diamond/gotostep/';

    jQuery(document).ready(function ($) {
        jQuery('.go-to-step-one').click(function (e) {
            e.preventDefault();
            goToStep(1);
        });

        jQuery('.go-to-step-two').click(function (e) {
            e.preventDefault();
            goToStep(2);
        });

        jQuery('.go-to-step-one-diamond').click(function (e) {
            e.preventDefault();
            var diamondType = jQuery(this).attr('diamond');
            if (diamondType) {
                goToStepDiamond(diamondType);
            }
        });

        jQuery('.go-to-step-two-setting').click(function (e) {
            e.preventDefault();
            var settingType = jQuery(this).attr('setting');

            if (settingType) {
                goToStepSetting(settingType);
            }
        });

        jQuery('.go-to-step-three-wedding').click(function (e) {
            e.preventDefault();
            var weddingType = jQuery(this).attr('wedding');
            var arr = weddingType.split('/');

            if (weddingType) {
                goToStepWedding(arr[0], arr[1]);
            }
        });

    });

    function goToStep(step) {
        jQuery.post(urlGoToStep, {step: step}, function (response) {
            response = JSON.parse(response);
            if (response.status) {
                if (step == 2) {
                    var redirectUrl = '<?php echo Mage::getBaseUrl() ?>engagement-rings.html';
                    redirectUrl += '?step=2';
                    window.location.href = redirectUrl;
                }
               else
                {
                var redirectUrl = '<?php echo Mage::getBaseUrl() ?>diamonds-1.html';    
                window.location.href = redirectUrl;
               }
            }
        });
    }

    function goToStepSetting(setting) {
        jQuery.post(urlGoToStep, {step: 2, style: setting}, function (response) {
            response = JSON.parse(response);
            if (response.status) {
                var redirectUrl = '<?php echo Mage::getBaseUrl() ?>engagement-rings.html?style=' + setting;
                window.location.href = redirectUrl;
            }
        });
    }

    function goToStepWedding(maincat, wedding) {
        jQuery.post(urlGoToStep, {step: 'wedding', maincat: maincat, weddingstyle: wedding}, function (response) {
            response = JSON.parse(response);
            if (response.status) {
                var redirectUrl = '<?php echo Mage::getBaseUrl() ?>wedding.html?maincat=' + maincat + '&weddingstep=' + wedding;
                window.location.href = redirectUrl;
            }
        });
    }

    function goToStepDiamond(diamond) {
        jQuery.post(urlGoToStep, {step: 1, shape: diamond}, function (response) {
            response = JSON.parse(response);
            if (response.status) {
                var redirectUrl = '<?php echo Mage::getBaseUrl() ?>diamonds-1.html?shape=' + diamond;
                window.location.href = redirectUrl;
            }
        });
    }

</script>